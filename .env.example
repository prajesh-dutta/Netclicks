# MongoDB Connection String
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/netclicks?retryWrites=true&w=majority

# NextAuth Configuration
NEXTAUTH_SECRET=your_nextauth_secret_at_least_32_chars_long
NEXTAUTH_URL=https://your-vercel-deployment-url.vercel.app

# OAuth Providers
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret

# Vercel specific
VERCEL_URL=${VERCEL_URL}